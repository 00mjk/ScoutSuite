    "display_path": "cloudtrail.regions.id",
    "path": "cloudtrail",
    "conditions": [
        "and",
        [
            "DuplicatedGlobalServiceEvents",
            "true",
            ""
        ]
    ],
    "id_suffix": "IncludeGlobalServiceEvents"
}
