{
    "description": "Lack of MFA (root Account)",
    "dashboard_name": "Root account",
    "path": "iam.credential_reports.id",
    "conditions": [
        "and",
        [
            "iam.credential_reports.id.mfa_active",
            "notTrue",
            ""
        ],
        [
            "iam.credential_reports.id.name",
            "equal",
            "<root_account>"
        ]
    ],
    "keys": [
        "this"
    ],
    "id_suffix": "mfa_active"
}