{
    "entities": [
        "iam.CredentialReport"
    ],
    "keys": [
        "this",
        "password_last_used",
        "access_key_1_last_used_date",
        "access_key_2_last_used_date",
        "user_creation_time"
    ],
    "conditions": [
	[ "password_last_used", "dateOlderThan", "180" ],
	[ "access_key_1_last_used_date", "dateOlderThan", "180" ],
	[ "access_key_2_last_used_date", "dateOlderThan", "180" ]
    ]
}
